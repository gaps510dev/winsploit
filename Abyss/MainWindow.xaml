            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RadioButton}">
                        <Border x:Name="border"  CornerRadius="15">
                            <Border.Background>
                                <LinearGradientBrush x:Name="name" Opacity="0" EndPoint="1.013,1.037" StartPoint="0.006,-0.032">
                                    <GradientStop Color="White"/>
                                    <GradientStop Color="#FFA4A4A4" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                            <Grid x:Name="templateRoot" Background="Transparent" SnapsToDevicePixels="True">
                                <Label x:Name="icon1" Content="󰅟" FontFamily="/Abyss;component/Fonts/#Material Design Icons" Foreground="Gray" FontSize="19" Margin="6,0,-10,-4" />
                                <Label x:Name="l1" Content="Scripts" FontFamily=" poppins Medium" Foreground="Gray" FontSize="12" Margin="44,0,-4,-4" />

                            </Grid>
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="True">
                                <Trigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>

                                            <ColorAnimation
                                    To= "#FF101010"
                                    Storyboard.TargetName="l1"
                                    Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                    FillBehavior="HoldEnd"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>
                                            <ColorAnimation
                                    To= "#FF101010"
                                    Storyboard.TargetName="icon1"
                                    Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                    FillBehavior="HoldEnd"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>
                                            <DoubleAnimation
                                    To="1"
                                    Storyboard.TargetName="name"
                                    Storyboard.TargetProperty="Opacity"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>

                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <ColorAnimation
                                    To= "Gray"
                                    Storyboard.TargetName="l1"
                                    Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                    FillBehavior="HoldEnd"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>
                                            <ColorAnimation
                                    To= "Gray"
                                    Storyboard.TargetName="icon1"
                                    Storyboard.TargetProperty="(Control.Foreground).(SolidColorBrush.Color)"
                                    FillBehavior="HoldEnd"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>
                                            <DoubleAnimation
                                    To="0"
                                    Storyboard.TargetName="name"
                                    Storyboard.TargetProperty="Opacity"
                                    Duration="0:0:0.25"
                                    AutoReverse="False"
                                    RepeatBehavior="1x"/>

                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.ExitActions>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
